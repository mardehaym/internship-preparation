#include <iostream>
using namespace std;

struct Node{
	int val;
	Node * left;
	Node * right;
	Node(int x) : val(x), right(NULL), left(NULL) {}
};

class Tree{
private:
	Node * _head;
public:
	Tree() {}
	void add(int x);
	void recAdd(Node * head, int& x);
};

void Tree::add(int x) {
	recAdd(_head, x;)
}

void Tree::recAdd(Node * head, int& x) {
	if (!head) {
		head = new Node(x);
	}
	if (x >= head->val) {
		recAdd(head->right, x);
	} else {
		recAdd(head->left, x);
	}
}

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
